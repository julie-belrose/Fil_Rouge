{
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "#backend/*": [
                "backend/src/*"
            ],
            "#config/*": [
                "backend/src/config/*"
            ],
            "#utils/*": [
                "backend/src/utils/*"
            ],
            "#scripts/*": [
                "backend/src/scripts/*"
            ],
            "#middleware/*": [
              "backend/src/middleware/*"
            ],
            "#domains/*": [
                "backend/src/domains/*"
            ],
            "#database/*": [
                "backend/src/database/*"
            ],
            "#loader/*": [
                "backend/src/loader/*"
            ],
            "#frontend/*": [
              "src/*"
            ],
            "#core/*": [
              "src/core/*"
            ],
            "#api/*": [
              "src/core/api/*"
            ],
            "#constants/*": [
              "src/constants/*"
            ],
            "#components/*": [
              "src/shared/components/*"
            ],
            "#domains-frontend/*": [
              "src/domains/*"
            ],
            "#styles/*": [
              "src/styles/*"
            ],
            "#public/*": [
              "src/public/*"
            ],
            "#shared-frontend/*": [
              "src/domains/shared/*"
            ],
            "#dashboard/*": [
              "src/domains/dashboard/*"
            ],
            "#errors-frontend/*": [
              "src/domains/errors/*"
            ],
            "#profile/*": [
              "src/domains/profile/*"
            ],
            "#report-frontend/*": [
              "src/domains/report/*"
            ],
            "#tests-frontend/*": [
              "tests/*"
            ],
            "#mock-frontend/*": [
              "src/tests/mock/*"
            ]
        }
    },
    "exclude": [
      "./node_modules",
      "dist"
    ]
}